{
	"version": 3,
	"routes": [
		{
			"src": "^/(.*)/$",
			"headers": {
				"Location": "/$1"
			},
			"status": 308
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/_server-islands/([^/]+?)$",
			"dest": "_render"
		},
		{
			"src": "^/_image$",
			"dest": "_render"
		},
		{
			"src": "^/404$",
			"dest": "_render"
		},
		{
			"src": "^/api/contact$",
			"dest": "_render"
		},
		{
			"src": "^/blog/([^/]+?)$",
			"dest": "_render"
		},
		{
			"src": "^/blog$",
			"dest": "_render"
		},
		{
			"src": "^/contact$",
			"dest": "_render"
		},
		{
			"src": "^/en/about$",
			"dest": "_render"
		},
		{
			"src": "^/en/blog/([^/]+?)$",
			"dest": "_render"
		},
		{
			"src": "^/en/blog$",
			"dest": "_render"
		},
		{
			"src": "^/en/contact$",
			"dest": "_render"
		},
		{
			"src": "^/en/services$",
			"dest": "_render"
		},
		{
			"src": "^/en/work/([^/]+?)$",
			"dest": "_render"
		},
		{
			"src": "^/en/work$",
			"dest": "_render"
		},
		{
			"src": "^/en$",
			"dest": "_render"
		},
		{
			"src": "^/es/about$",
			"dest": "_render"
		},
		{
			"src": "^/es/contact$",
			"dest": "_render"
		},
		{
			"src": "^/es/services$",
			"dest": "_render"
		},
		{
			"src": "^/es$",
			"dest": "_render"
		},
		{
			"src": "^/privacy$",
			"dest": "_render"
		},
		{
			"src": "^/robots\\.txt/?$",
			"dest": "_render"
		},
		{
			"src": "^/rss\\.xml/?$",
			"dest": "_render"
		},
		{
			"src": "^/terms$",
			"dest": "_render"
		},
		{
			"src": "^/work/([^/]+?)$",
			"dest": "_render"
		},
		{
			"src": "^/work$",
			"dest": "_render"
		},
		{
			"src": "^/$",
			"dest": "_render"
		},
		{
			"src": "^/.*$",
			"dest": "_render",
			"status": 404
		}
	]
}